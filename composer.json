{
    "name": "acidf0x/laravel-ez-throttle",
    "description": "A simple Laravel Throttle extension",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "duhuicho",
            "email": "ic_jdh@me.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "AcidF0x\\EzThrottle\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "AcidF0x\\EzThrottle\\EzThrottleServiceProvider"
        ]
      }
    },
    "require": {
      "php": ">=7.0",
      "laravel/framework": "5.5"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
